<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SunMoveCircleImageView">
        <attr name="sciv_border_width" format="dimension" />
        <attr name="sciv_border_color" format="color" />
        <attr name="sciv_border_overlay" format="boolean" />
        <attr name="sciv_fill_color" format="color" />
    </declare-styleable>

    <!-- 自定义，自动换行的横向布局 -->
    <declare-styleable name="AutoLineLayout">
        <attr name="itemSpace" format="dimension"></attr>
        <attr name="lineSpace" format="dimension"></attr>
    </declare-styleable>
</resources>